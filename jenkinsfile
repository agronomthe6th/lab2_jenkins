pipeline {
    agent none

    stages {
        stage('Build') {
            agent {
                docker { image 'gcc:latest' }
            }
            steps {
                checkout scm
                script {
                    // Ваши шаги сборки
                    sh 'make'
                }
            }
        }
    }

    post {
        success {
            // Ваши действия в случае успешной сборки
            archiveArtifacts 'path/to/artifacts/*'
        }
    }
}
